{
  "type": "module",
  "dependencies": {
    "@coinbase/agentkit": "^0.4.0",
    "@coinbase/agentkit-langchain": "^0.3.0",
    "@langchain/core": "^0.3.42",
    "@langchain/langgraph": "^0.2.57",
    "@langchain/openai": "^0.4.7",
    "@recallnet/chains": "^0.0.6",
    "@recallnet/contracts": "^0.0.4",
    "@recallnet/sdk": "^0.0.7",
    "cborg": "^4.2.9",
    "dotenv": "^16.4.7",
    "node-telegram-bot-api": "^0.66.0",
    "reflect-metadata": "^0.2.2",
    "viem": "^2.23.14"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "@types/node-telegram-bot-api": "^0.64.8",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node --experimental-modules --es-module-specifier-resolution=node dist/chatbot.js",
    "lint": "eslint \"src/**/*.ts\" \"src/action-providers/xocolatl/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"src/action-providers/xocolatl/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"src/action-providers/xocolatl/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"src/action-providers/xocolatl/**/*.{ts,js,json}\"",
    "check": "tsc --noEmit",
    "test": "npx jest --no-cache --testMatch='**/*.test.ts'",
    "test:dry-run": "npm install && npm ci && npm publish --dry-run",
    "test:e2e": "npx jest --no-cache --testMatch=**/e2e.ts --coverageThreshold '{}'",
    "test:types": "tsd --files src/tests/types.test-d.ts",
    "prepack": "tsc",
    "docs": "typedoc --entryPoints ./src --entryPointStrategy expand --exclude ./src/tests/**/*.ts",
    "docs:serve": "http-server ./docs",
    "moderia:demo": "node dist/action-providers/moderia/run-demo.js",
    "moderia:terminal": "node dist/action-providers/moderia/demo-terminal.js",
    "moderia:telegram": "node dist/action-providers/moderia/demo-telegram.js",
    "moderia:test": "node dist/action-providers/moderia/test.js"
  }
}
